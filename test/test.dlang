import 'dl:main/database.dlang'

var dbname = 'cat'
var omghi = 'dog'

createUnsafeDB(dbname)
createDocument(dbname, 'helo!')
createProperty(dbname, 'helo!', omghi, 'does this work/')
createProperty(dbname, 'bibi!', omghi, 'does this work also\')
removeProperty(dbname, 'helo!', omghi)
renameDocument(dbname, 'bibi!', 'heysup')
renameProperty(dbname, 'heysup', 'dog', 'hey sup!')
changeProperty(dbname, 'heysup', 'hey sup!', 'ok sure how are you?')